{
    "version": 2,
    "builds": [{
            "src": "api/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        { "src": "/api/user/([^/]+)$", "dest": "/api/getUser.js" },
        { "src": "/api/user/([^/]+)/open-pack$", "dest": "/api/openPack.js" },
        { "src": "/api/packs$", "dest": "/api/packs.js" },
        { "src": "/(.*)", "dest": "/public/$1" }
    ]
}